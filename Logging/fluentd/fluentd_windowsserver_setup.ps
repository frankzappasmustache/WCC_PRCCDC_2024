# Download Fluentd
Invoke-WebRequest -Uri "https://www.fluentd.org/downloads/1.14/fluentd-1.14.2-x64.msi" -OutFile "fluentd.msi"

# Install Fluentd
Start-Process msiexec.exe -ArgumentList "/i fluentd.msi /quiet" -Wait

# Install Fluentd as a service
fluentd.exe install -c "C:\path\to\your\fluent.conf"

# Start Fluentd service
Start-Service fluentd
